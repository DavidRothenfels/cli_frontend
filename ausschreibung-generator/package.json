{
  "name": "vergabedokument-generator",
  "version": "1.0.0",
  "description": "Automated German procurement document generator",
  "main": "process_cli_commands.js",
  "scripts": {
    "start": "node process_cli_commands.js",
    "dev": "./run.sh",
    "docker:build": "docker build -t vergabedokument-generator .",
    "docker:run": "docker run -p 8090:8090 -v $(pwd)/pb_data:/app/pb_data vergabedokument-generator"
  },
  "keywords": [
    "procurement",
    "documents",
    "pocketbase",
    "ai",
    "government"
  ],
  "author": "Vergabedokument-Generator Team",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^3.3.2",
    "pocketbase": "^0.26.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/vergabedokument-generator.git"
  }
}
